{
  "name": "pinguinag-freight",
  "version": "2.1.3",
  "author": "Pinguin AG forked from Puppet-Finland team",
  "summary": "Setup freight, an apt repository management tool",
  "license": "BSD-2-Clause",
  "source": "https://github.com/ag-pinguin/puppet-freight",
  "project_page": "https://github.com/ag-pinguin/puppet-freight",
  "dependencies": [
    {
      "name": "puppetfinland/os",
      "version_requirement": ">= 0.5.5"
    },
    {
      "name": "puppetfinland/nginx",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
